{
  "name": "@enpitsulin/gatsby-remark-shiki",
  "description": "Adds syntax highlighting to code blocks with gatsby-transform-remark by using shiki",
  "version": "0.0.5",
  "main": "index.js",
  "typing": "index.d.ts",
  "license": "MIT",
  "author": "enpitsulin",
  "repository": {
    "url": "https://github.com/enpitsuLin/gatsby-remark-shiki"
  },
  "keywords": [
    "gatsby",
    "remark",
    "gatsby-plugin",
    "shiki"
  ],
  "scripts": {
    "build": "babel src --out-dir dist --extensions .ts",
    "build:type": "tsc",
    "dev": "babel -w src --out-dir dist --extensions .ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "babel-preset-gatsby-package": "^2.0.0",
    "shiki": "^0.9.12",
    "unist-util-visit": "^2.0.1"
  }
}
